{
  "version": 1,
  "formatters": {
    "json": {
      "format": "%(asctime)s %(levelname)s %(name)s %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "json",
      "level": "INFO"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console"]
  },
  "loggers": {
    "uvicorn.access": {"handlers": ["console"], "level": "INFO", "propagate": false},
    "uvicorn.error": {"handlers": ["console"], "level": "INFO", "propagate": false}
  }
}
